{
  "name": "@nivashr/vue-hotel-datepicker",
  "version": "2.3.0",
  "description": "A customizable hotel date-picker component for Vue 2 (forked and improved)",
  "main": "lib/vue-hotel-datepicker.umd.js",
  "module": "lib/vue-hotel-datepicker.esm.js",
  "unpkg": "lib/vue-hotel-datepicker.min.js",
  "browser": {
    "./sfc": "src/components/VueHotelDatepickerModal.vue"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:lib": "vue-cli-service build --target lib --dest lib  --name vue-hotel-datepicker ./src/components/VueHotelDatepicker.vue",
    "build:wc": "vue-cli-service build --target wc --dest wc --name vue-hotel-datepicker ./src/components/VueHotelDatepicker.vue",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit --coverage --no-cache",
    "prebuild": "vue-cli-service build --target lib --dest lib  --name vue-hotel-datepicker ./src/components/VueHotelDatepicker.vue",
    "postbuild": "rm -rf ./docs && cp -rf ./dist ./docs "
  },
  "author": "Northwalker <northwalker28@gmail.com>",
  "contributors": [
    {"name":"Neven Jacmenović","url":"https://github.com/guycalledseven"},
    {"name":"Rudolf Jozanović","url":"https://github.com/nivas"}
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nivas/vue-hotel-datepicker.git"
  },
  "keywords": [
    "vue",
    "vue2",
    "vuejs",
    "datepicker",
    "date picker",
    "date range picker",
    "hotel datepicker",
    "vue datepicker",
    "vue hotel datepicker"
  ],
  "bugs": {
    "url": "https://github.com/nivas/vue-hotel-datepicker/issues"
  },
  "homepage": "https://github.com/nivas/vue-hotel-datepicker#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-e2e-cypress": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-plugin-pwa": "^3.10.0",
    "@vue/cli-plugin-unit-jest": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.69.5",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "tests"
  }
}
